{
  "name": "care_provider",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "genimg": "node genimg src/assets/images/source --output src/assets/images/index.ts --name images",
    "genicon": "node genimg src/assets/icons/source --output src/assets/icons/index.ts --name icons",
    "m1": "  cd ios && rm -rf build && xcodebuild clean && cd .. && arch -x86_64 react-native run-ios --simulator=\"iPhone 14 Pro\"",
    "android-release": "npx react-native run-android --variant=release",
    "format": "prettier --write './src/**/*.{ts,tsx,js,json,md}'",
    "link": "npx react-native link",
    "apk": "cd android &&./gradlew clean && ./gradlew app:assembleRelease && cd ..",
    "aab": "cd android &&./gradlew clean && ./gradlew bundleRelease && cd ..",
    "p1": "cd ios && arch -x86_64 pod install && cd ..",
    "p1u": "cd ios && arch -x86_64 pod install --repo-update && cd ..",
    "pod": "cd ios && pod install && cd ..",
    "clean": "cd ios && rm -rf build && xcodebuild clean && cd ..",
    "cphandroid": "appcenter codepush release-react -a SmartPower-android -d Production",
    "i15": "react-native run-ios --simulator=\"iPhone 15 Pro\""
  },
